type AccountJournal {
  id: ID!
  businessId: String!
  branch: AllBranch! @goField(forceResolver: true)
  transactionDateTime: Time!
  transactionNumber: String
  transactionDetails: String
  referenceNumber: String
  customer: Customer
  supplier: Supplier
  referenceId: Int
  referenceType: AccountReferenceType
  accountTransactions: [AccountTransaction]
  referenceData: AccountJournalReferenceUnion! @goField(forceResolver: true)
}

union AccountJournalReferenceUnion =
    Journal
  | SalesInvoice
  | PaidInvoice
  | CreditNote
  | Bill
  | SupplierPayment
  | SupplierPaidBill

# already add 'JN'/Journal,'IV'/SalesInvoice,'IP'/PaidInvoice,'CN'/CreditNote,'BL'/Bill,'SP'/SupplierPayment,BP'/SupplierPaidBill
# need to add 'CNR','EP','IVT','IVA','WO','ACP','ASP','COB','SOB','OB'

